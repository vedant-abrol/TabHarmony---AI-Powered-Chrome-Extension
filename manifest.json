
{
  "manifest_version": 3,
  "name": "TabHarmony",
  "version": "1.0.0",
  "description": "Intelligent tab organization powered by AI",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
