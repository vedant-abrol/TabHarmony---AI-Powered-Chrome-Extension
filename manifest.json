
{
  "manifest_version": 3,
  "name": "TabHarmony",
  "version": "1.0.0",
  "description": "Intelligent tab organization powered by AI",
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "host_permissions": [
    "https://api.groq.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
